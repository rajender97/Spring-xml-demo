<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="actor" class="com.stackroute.ActorName">
        <constructor-arg index="0" value="Rajender"> </constructor-arg>
        <constructor-arg index="1" value="male"> </constructor-arg>
        <constructor-arg index="2" value="22"></constructor-arg>

    </bean>

    <bean id="actor1" class="com.stackroute.ActorName">
        <constructor-arg index="0" value="Santhosh"> </constructor-arg>
        <constructor-arg index="1" value="male"> </constructor-arg>
        <constructor-arg index="2" value="17"></constructor-arg>

    </bean>




    <bean id="movie1" autowire="byName" class="com.stackroute.MovieName" >

    </bean>
    <bean id="movie2" autowire="byName" class="com.stackroute.MovieName" >

    </bean>


</beans>
